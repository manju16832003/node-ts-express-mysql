{
  "name": "ts-express-mysql",
  "version": "1.0.0",
  "description": "Node TypeScrpt Express MySQL Starter",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run && npm run build-ts && npm run tslint",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manju16832003/node-ts-express-mysql.git"
  },
  "keywords": [
    "node",
    "ts",
    "express",
    "mysql"
  ],
  "author": "Manjunath Reddy <manju16832003@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/manju16832003/node-ts-express-mysql/issues"
  },
  "homepage": "https://github.com/manju16832003/node-ts-express-mysql#readme",
  "engines": {
    "node": "10.4.1"
  },
  "dependencies": {
    "@types/errorhandler": "0.0.32",
    "dotenv": "6.0.0",
    "errorhandler": "1.5.0",
    "express": "4.16.3"
  },
  "devDependencies": {
    "@types/express": "4.16.0",
    "@types/jest": "23.1.0",
    "jest": "23.1.0",
    "nodemon": "1.17.5",
    "tslint": "5.10.0",
    "typescript": "2.9.2"
  }
